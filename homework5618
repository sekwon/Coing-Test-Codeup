n = int(input())

if n==2:
    num1,num2=map(int, input().split())  
    data=[]
    for i in range(1, num1+1):
       if(num1 % i == 0) & (num2 % i ==0):
         data.append(i)
    print (num1, data)
    print (num2, data)

if n==3:   
    num1,num2,num3=map(int, input().split())
    data=[]
    for i in range(1, num1+1):
       if(num1 % i == 0) & (num2 % i ==0) & (num3 % i==0):
         data.append(i)

    print (num1, data)
    print (num2, data)
    print (num3, data)
